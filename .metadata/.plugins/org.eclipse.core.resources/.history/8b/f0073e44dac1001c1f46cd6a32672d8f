
public class AtividadePratica {

	public static void main(String entrada[]) {
		
		
		int n1, n2, soma;
		
		
		
		n1= Integer.parseInt(entrada[1]);
		n2= Integer.parseInt(entrada[2]);
		
	
		
		soma = n1 +n2;
		
		
		
		System.out.println(n1 + " + " + n2 + " = " + soma);
		System.exit(0);


	}

}
