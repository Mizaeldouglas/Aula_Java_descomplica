
public class AtividadePratica {

	public static void main(String entrada[]) {
		//Declara√ßao de variaveis
		
		int n1, n2, soma;
		
		//entradas de dados
		
		n1= Integer.parseInt(entrada[0]);
		n2= Integer.parseInt(entrada[1]);
		
		//Processo
		
		soma = n1 +n2;
		
		//Saida de resultado
		
		System.out.println(n1 + " + " + n2 + " = " + soma);
		System.exit(0);


	}

}
